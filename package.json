{
  "name": "ssam",
  "version": "0.8.7",
  "description": "Creative coding sketch helper",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "type": "module",
  "files": [
    "/dist"
  ],
  "scripts": {
    "demo": "vite ./demo --config vite.demo.config.ts",
    "prebuild": "license-checker --exclude 'MIT, MIT OR X11, BSD, ISC' -production",
    "build": "tsc --noemit && tsup",
    "build-vite": "tsc --noemit && vite build --config vite.build.config.ts",
    "preview": "vite preview",
    "watch": "tsup --watch"
  },
  "author": "Daeinc",
  "license": "MIT",
  "dependencies": {
    "@daeinc/canvas": "^0.14.5",
    "@daeinc/dom": "^0.4.1",
    "gifenc": "^1.0.3",
    "webm-muxer": "^1.2.1"
  },
  "devDependencies": {
    "@daeinc/color": "^0.2.4",
    "@daeinc/draw": "^0.4.0",
    "@types/node": "^18.13.0",
    "@types/ogl": "npm:ogl-types@^0.0.102",
    "@types/p5": "^1.5.0",
    "animejs": "^3.2.1",
    "license-checker": "^25.0.1",
    "ogl": "^0.0.112",
    "p5": "^1.6.0",
    "pts": "^0.11.3",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-ssam-export": "^0.1.0",
    "vite-plugin-ssam-ffmpeg": "^0.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdaein/ssam.git"
  },
  "keywords": [
    "creative-coding"
  ],
  "bugs": {
    "url": "https://github.com/cdaein/ssam/issues"
